      </Row>
      <Row>
        <Cell><Data ss:Type="String">    name VARCHAR(100),</Data></Cell>
      </Row>
      <Row>
        <Cell><Data ss:Type="String">    email VARCHAR(100),</Data></Cell>
      </Row>
      <Row>
        <Cell><Data ss:Type="String">    phone_number VARCHAR(20),</Data></Cell>
      </Row>
      <!-- Add other student-related fields here -->
